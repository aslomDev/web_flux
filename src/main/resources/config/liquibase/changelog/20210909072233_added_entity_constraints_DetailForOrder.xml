<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity DetailForOrder.
    -->
    <changeSet id="20210909072233-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="order_id"
                                 baseTableName="detail_for_order"
                                 constraintName="fk_detail_for_order__order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_order"/>

        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="detail_for_order"
                                 constraintName="fk_detail_for_order__product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>
    </changeSet>
</databaseChangeLog>
